{
  "name": "schedule-connector",
  "version": "1.0.0",
  "description": "Starter application of Angular2 on Express",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "start": "node ./server/bin/www",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "devinstall": "typings install && tsc -p ./server && tsc -p ./client",
    "compile": "tsc -p ./client && tsc -p ./server || true"
  },
  "engines": {
    "node": "^7.2.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@angular2-material/core": "^2.0.0-alpha.8-2",
    "angular2-in-memory-web-api": "0.0.20",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^5.7.0",
    "ng2-material": "^0.8.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "typings": "^1.3.2",
    "typescript": "^2.0.2",
    "angular2-jwt": "^0.1.18",
    "concurrently": "^2.2.0",
    "nodemon": "^1.9.1",
    "systemjs-builder": "^0.15.26",
    "yargs": "^4.8.1"
  }
}
